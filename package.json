{
  "name": "debug-ui",
  "version": "0.0.1",
  "description": "Simple ui components for logging to browser.",
  "browser": "dist/index.bundle.js",
  "main": "dist/index.js",
  "types": "dist/dts/index.d.ts",
  "scripts": {
    "clean": "rimraf dist coverage demo",
    "typecheck": "tsc -p . --noEmit",
    "lint": "tslint --project tsconfig.json --format stylish",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "demo": "rollup --environment BUNDLE:true --config rollup.config.js --configDemo",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "posttest": "npm run typecheck && npm run lint",
    "build": "npm-run-all --aggregate-output --parallel build:*",
    "build:standalone": "rollup --environment BUNDLE:false --config rollup.config.js --sourcemap",
    "build:esm": "rollup --environment ESM:true,BUNDLE:false --config rollup.config.js --sourcemap",
    "build:bundle": "rollup --environment BUNDLE:true --config rollup.config.js --sourcemap",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "lint-staged",
    "postcommit": "git reset"
  },
  "keywords": [
    "ui",
    "debug",
    "logger",
    "typescript",
    "lib",
    "library"
  ],
  "files": [
    "dist"
  ],
  "author": "Ivan Mucyo Ngabo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.6",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.union": "^4.6.7",
    "@types/lodash.uniqueid": "^4.0.7",
    "autoprefixer": "^10.4.7",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-css": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-csso": "^6.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-dev": "^2.0.4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.union": "^4.6.0",
    "lodash.uniqueid": "^4.0.1"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "tslint --fix",
        "git add"
      ]
    }
  }
}
